{
  "name": "scryfall-image-downloader",
  "version": "1.0.0",
  "description": "Small tool for downloading scryfall images",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "rm -rf release & npm run compile",
    "build": "pkg build/index.js --target=node14-win-x64 -o release/scryfall-image-downloader",
    "postbuild": "copyfiles cards.txt app.properties release",
    "compile": "tsc",
    "start": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gikkman/scryfall-image-downloader.git"
  },
  "author": "Gikkman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gikkman/scryfall-image-downloader/issues"
  },
  "homepage": "https://github.com/Gikkman/scryfall-image-downloader#readme",
  "dependencies": {
    "properties-reader": "^2.2.0",
    "scryfall-api": "^2.2.2"
  },
  "devDependencies": {
    "@types/properties-reader": "^2.1.1",
    "copyfiles": "^2.4.1",
    "pkg": "^5.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
